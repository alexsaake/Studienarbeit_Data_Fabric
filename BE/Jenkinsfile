pipeline {
    agent any

    stages {

        stage('build') {
            steps {
                sh 'mvnw --version'
            }
        }

        stage("Compilation") {
           steps {
                sh "mvnw clean install -DskipTests"
            }
        }
        stage("Tests and Deployment") {
/*             steps {
                echo 'Runing unit tests'
                //sh "./mvnw test -Punit"
             } */
            steps {
                echo 'Deployment'
                sh 'mvnw spring-boot:run'
             }
         }
    }
}